{
  "name": "root",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "compile": "node scripts/compile.js",
    "ts:clean": "find . -name \"*.tsbuildinfo\" -type f -delete && rm -rf packages/*/dist",
    "fell": "yarn ts-node --transpile-only --project ./packages/fell/tsconfig.json ./packages/fell/src/app.ts",
    "fell:fresh-clone": "rm -rf ./en && rm -rf ./es && mkdir -p ./en ./es && yarn compile fell && yarn fell clone",
    "fl": "yarn ts-node --transpile-only --project ./packages/cli/tsconfig.json ./packages/cli/src/app.ts",
    "test": "yarn jest",
    "lint": "yarn ts:lint",
    "ts:lint": "yarn eslint 'packages/**/*.{ts,tsx}'",
    "asciidoc:scaffold:lint": "cd ./packages/adoc-lint && yarn scaffold:lint",
    "styleguide:develop": "cd ./packages/styleguide && yarn start",
    "styleguide:build": "cd ./packages/styleguide && yarn build",
    "evans:build:en": "cd packages/evans && yarn build:en",
    "evans:build:es": "cd packages/evans && yarn build:es",
    "evans:develop:en": "cd packages/evans && yarn develop:en",
    "evans:develop:es": "cd packages/evans && yarn develop:es",
    "evans:develop:en:site": "cd packages/evans && yarn develop:en:site",
    "evans:develop:es:site": "cd packages/evans && yarn develop:es:site",
    "en": "yarn evans:develop:en:site",
    "es": "yarn evans:develop:es:site",
    "evans:ts:check": "cd packages/evans && yarn ts:check",
    "jones:develop": "cd packages/jones && yarn develop",
    "jones:build": "cd packages/jones && yarn build",
    "cover:develop": "cd packages/cover-web-app && yarn develop",
    "cover:build": "cd packages/cover-web-app && yarn build",
    "cover:ts:check": "cd packages/cover-web-app && yarn ts:check",
    "test:integration": "cd packages/functions && yarn test:integration && cd ../ui && yarn test:integration",
    "bot:deploy": "yarn compile bot && cd ./packages/bot && yarn deploy",
    "bot:develop": "node ./packages/bot/dist/scripts/run.js",
    "bot:receive": "cd packages/bot && yarn receive",
    "format:yml": "yarn prettier \"packages/friends/**/*.yml\" --write",
    "format:all": "yarn prettier \"packages/**/*.{ts,tsx,js,css,scss,yml}\" --write",
    "storybook": "cd packages/ui && yarn storybook",
    "ui:start": "cd packages/ui && yarn storybook",
    "ts:check": "yarn evans:ts:check && yarn cover:ts:check && yarn compile all",
    "kick:tires": "yarn lint && yarn test && yarn ts:check",
    "netlify:cache": "[ $NETLIFY = true ] && ./scripts/netlify-yarn-cache.sh || echo \"skip netlify:cache\"",
    "netlify:restore": "[ $NETLIFY = true ] && ./scripts/netlify-yarn-restore.sh || echo \"skip netlify:restore\"",
    "preinstall": "YARN_SILENT=0 yarn netlify:restore",
    "algolia:send": "FORCE_ALGOLIA_SEND=true yarn ts-node packages/evans/src/build/algolia.ts",
    "algolia:send:es": "GATSBY_LANG=es yarn algolia:send",
    "algolia:send:en": "GATSBY_LANG=en yarn algolia:send",
    "preflight": "yarn lint && yarn fl make en/robert-barclay/recreation-entertainment/updated --target all -o && yarn test && yarn evans:ts:check && yarn cover:ts:check && yarn ts:clean && yarn compile all && yarn styleguide:build && yarn evans:build:en && yarn evans:build:es && yarn jones:build && yarn cover:build && yarn fl check-intake"
  },
  "dependencies": {
    "lodash": "^4.17.15",
    "prettier": "^1.16.4",
    "typescript": "3.8.3"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.123",
    "@typescript-eslint/eslint-plugin": "2.x",
    "@typescript-eslint/parser": "2.x",
    "babel-eslint": "10.x",
    "eslint": "6.x",
    "eslint-config-prettier": "^4.1.0",
    "eslint-config-react-app": "^5.2.1",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-flowtype": "4.x",
    "eslint-plugin-import": "2.x",
    "eslint-plugin-jsx-a11y": "6.x",
    "eslint-plugin-no-only-tests": "^2.3.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-react": "7.x",
    "eslint-plugin-react-hooks": "2.x",
    "eslint-plugin-standard": "^4.0.0",
    "jest": "^25.0.0",
    "lerna": "^3.1.4",
    "stylelint": "^9.5.0",
    "stylelint-config-recommended": "^2.1.0",
    "ts-jest": "^25.0.0",
    "webpack-cli": "^3.2.3"
  },
  "workspaces": [
    "packages/*"
  ]
}
