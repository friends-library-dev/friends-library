{
  "name": "@friends-library/cover",
  "version": "1.0.0",
  "private": true,
  "main": "dist/index.js",
  "dependencies": {
    "@friends-library/adoc-utils": "1.0.0",
    "@friends-library/cover-component": "1.0.0",
    "@friends-library/document-meta": "1.0.0",
    "@friends-library/cli-utils": "1.0.0",
    "@friends-library/env": "1.0.0",
    "@friends-library/friends": "1.0.0",
    "@friends-library/types": "1.0.0",
    "@material-ui/core": "^4.0.0-beta.0",
    "@material-ui/icons": "^4.0.0-beta.0",
    "brace": "^0.11.1",
    "browser-or-node": "^1.2.1",
    "classnames": "^2.2.6",
    "glob": "^7.1.3",
    "lodash": "^4.17.11",
    "react": "^16.9.0",
    "react-ace": "^6.5.0",
    "react-contenteditable": "^3.2.6",
    "react-dom": "^16.8.6",
    "react-keyboard-event-handler": "^1.4.1",
    "react-scripts": "3.0.0",
    "ts-node": "^8.1.0"
  },
  "scripts": {
    "compile": "rm -rf ./dist && mkdir ./dist && cp -r ./public/images ./dist && cd ../../ && yarn tsc --build packages/cover/src/components/Cover",
    "ts:check": "../../node_modules/.bin/tsc --noEmit -p . && ../../node_modules/.bin/tsc --noEmit -p ./src/components/Cover",
    "listen:pdf": "node ./listen/listen-pdf.js",
    "develop": "yarn friends && concurrently \"yarn compile -w\" yarn:listen:pdf yarn:start \"sleep 3 && cd ../../ && yarn kite cover:watch\"",
    "start": "BROWSER=none SKIP_PREFLIGHT_CHECK=true PORT=9999 react-scripts start",
    "friends": "yarn ts-node -T --skip-project ./src/scripts/friend-data.ts",
    "build": "yarn friends && SKIP_PREFLIGHT_CHECK=true react-scripts build",
    "clone:netlify": "cd ../../ && mkdir -p ./en && mkdir -p ./es && yarn compile fell && yarn fell clone",
    "build:netlify": "yarn clone:netlify && cd ../../ && yarn tsc --build packages/cover && cd packages/cover && yarn build",
    "test": "cd ../../ && yarn jest packages/cover",
    "eject": "react-scripts eject"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/jest": "24.0.11",
    "@types/node": "11.13.7",
    "@types/react": "^16.9.4",
    "@types/react-dom": "16.8.4",
    "concurrently": "^4.1.0"
  }
}
