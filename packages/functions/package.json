{
  "name": "@friends-library/functions",
  "version": "1.0.0",
  "description": "serverless functions in the cloud!",
  "main": "dist/index.js",
  "author": "Jared Henderson <jared@netrivet.com>",
  "license": "MIT",
  "scripts": {
    "start": "NODE_ENV=development yarn netlify-lambda serve src --config ./webpack.config.js --port 2345",
    "build": "NODE_ENV=production yarn netlify-lambda build src --config ./webpack.config.js",
    "test": "cd ../../ && yarn jest packages/functions",
    "test:integration": "cd ../../ && yarn test --testRegex=packages/functions/.*/.*\\.integration\\.ts --env=node"
  },
  "dependencies": {
    "@babel/plugin-proposal-class-properties": "^7.7.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.6.2",
    "@babel/plugin-proposal-optional-chaining": "^7.7.5",
    "@babel/plugin-transform-object-assign": "^7.2.0",
    "@babel/preset-typescript": "^7.3.3",
    "@babel/runtime": "^7.7.2",
    "@friends-library/env": "1.0.0",
    "@friends-library/slack": "1.0.0",
    "@friends-library/types": "1.0.0",
    "@sendgrid/mail": "^6.4.0",
    "bufferutil": "^4.0.1",
    "client-oauth2": "^4.2.5",
    "express-useragent": "^1.0.12",
    "gmail-send": "^1.2.14",
    "jsonschema": "^1.2.5",
    "mongoose": "^5.6.4",
    "netlify-lambda": "^1.5.0",
    "node-fetch": "^2.6.0",
    "stripe": "^7.5.2",
    "utf-8-validate": "^5.0.2",
    "webpack": "^4.35.3",
    "ws": "^7.1.0"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@types/aws-lambda": "^8.10.28",
    "@types/express-useragent": "^0.2.21",
    "@types/mongoose": "^5.5.8",
    "@types/stripe": "^6.31.11"
  }
}
