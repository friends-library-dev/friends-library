  "scripts": {
    "test": "cd ../../ && yarn jest packages/evans",
    "ts:check": "../../node_modules/.bin/tsc --noEmit -p .",
    "develop:functions": "cd ../functions && yarn start",
    "compile": "cd ../../ && yarn compile evans",
    "build:functions": "cd ../functions && yarn build",
    "build": "gatsby clean && gatsby build && yarn build:functions",
    "build:en": "GATSBY_LANG=en yarn build",
    "build:es": "GATSBY_LANG=es yarn build",
    "dpc-cache:clean": "rm -f src/build/.dpc-cache.json",
    "develop:en:site": "GATSBY_LANG=en GATSBY_PORT=9222 GATSBY_ALT_PORT=9333 gatsby develop --port 9222",
    "develop:es:site": "GATSBY_LANG=es GATSBY_PORT=9333 GATSBY_ALT_PORT=9222 gatsby develop --port 9333",
    "develop:en": "run-p develop:en:site develop:functions",
    "develop:es": "run-p develop:es:site develop:functions",
    "build:netlify": "yarn compile && yarn build && ln -s ../functions/dist functions",
    "po:update": "yarn ttag update es.po src"
  },
  "devDependencies": {
    "@types/he": "^1.1.0",
    "@types/mdx-js__react": "^1.5.0",
    "@types/react": "16.9.11",
    "@types/react-dom": "^16.8.3",
    "@types/react-helmet": "^5.0.11",
    "@types/smoothscroll-polyfill": "^0.3.1",
    "eslint-config-react-app": "^3.0.8"
  }
