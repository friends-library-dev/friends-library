{
  "name": "static",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "private": false,
  "scripts": {
    "test": "jest src",
    "test:watch": "jest src --watch",
    "start": "yarn start:en",
    "start:en": "yarn sync:en & LANG=en NODE_ENV=development PORT=2222 API_URL=http://localhost:1111 NODE_PATH=src babel-watch src/server/index.js & webpack --mode development --watch",
    "start:es": "yarn sync:es & LANG=es NODE_ENV=development PORT=3333 API_URL=http://localhost:1111 NODE_PATH=src babel-watch --watch es.po src/server/index.js & webpack --mode development --watch",
    "serve": "NODE_ENV=production PORT=4444 NODE_PATH=src babel-watch src/server/index.js",
    "prebuild": "webpack --mode production -p",
    "build": "rimraf build && NODE_PATH=src babel-node build.js",
    "build:en": "rimraf build && LANG=en NODE_PATH=src babel-node build.js",
    "build:es": "rimraf build && LANG=es NODE_PATH=src babel-node build.js",
    "sync:en": "browser-sync start --port 2223 --ui-port 2224 --files src/* --no-notify",
    "sync:es": "browser-sync start --port 2225 --ui-port 2226 --files src/* es.po --no-notify",
    "flow-typed:update": "yarn flow-typed install --overwrite --ignoreDeps dev",
    "lint:js": "eslint src storybook",
    "fix:js": "eslint src storybook --fix",
    "storybook": "start-storybook -p 6006 -c .storybook",
    "preflight": "yarn lint:js && yarn test && yarn flow",
    "po:update": "yarn c-3po update es.po src"
  },
  "dependencies": {
    "@friends-library/friends": "git://github.com/friends-library/friends.git#083369b92a38a1ea11645b535a06dc284004c718",
    "c-3po": "^0.8.0",
    "clean-css": "^4.1.9",
    "express": "^4.16.2",
    "gettext-parser": "^1.3.1",
    "glamor": "^2.20.40",
    "glob": "^7.1.2"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.3.9",
    "@storybook/react": "^3.3.9",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.26.0",
    "babel-watch": "^2.0.7",
    "browser-sync": "^2.23.3",
    "c-3po-cli": "^0.1.1",
    "eslint": "^4.13.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-flowtype": "^2.35.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-no-only-tests": "^2.0.0",
    "eslint-plugin-react": "^7.4.0",
    "flow-bin": "^0.74.0",
    "flow-typed": "^2.4.0",
    "fs-extra": "^5.0.0",
    "jest": "^23.0.0",
    "js-yaml": "^3.10.0",
    "react": "^16.4.0",
    "react-dom": "^16.3.2",
    "rimraf": "^2.6.2",
    "webpack": "^4.6.0",
    "webpack-cli": "^2.0.14",
    "yaml-frontmatter-loader": "^0.1.0"
  },
  "jest": {
    "modulePaths": [
      "<rootDir>/src"
    ]
  }
}
