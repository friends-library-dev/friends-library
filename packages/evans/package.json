  "scripts": {
    "test": "cd ../../ && yarn jest packages/evans",
    "ts:check": "../../node_modules/.bin/tsc --noEmit -p .",
    "build:en": "GATSBY_LANG=en yarn gatsby build",
    "build:es": "GATSBY_LANG=es yarn gatsby build",
    "develop:en": "yarn concurrently yarn:develop:functions:en yarn:develop:site:en",
    "develop:es": "yarn concurrently yarn:develop:functions:es yarn:develop:site:es",
    "develop:functions:en": "GATSBY_LANG=en yarn netlify-lambda serve src/functions",
    "develop:functions:es": "GATSBY_LANG=es yarn netlify-lambda serve src/functions",
    "develop:site:en": "GATSBY_LANG=en GATSBY_PORT=9222 gatsby develop --port 9222",
    "develop:site:es": "GATSBY_LANG=es GATSBY_PORT=9222 gatsby develop --port 9222",
    "po:update": "yarn ttag update es.po src"
  },
  "devDependencies": {
    "@types/he": "^1.1.0",
    "@types/react": "^16.8.10",
    "@types/react-dom": "^16.8.3",
    "@types/react-helmet": "^5.0.8",
    "eslint-config-react-app": "^3.0.8"
  }
