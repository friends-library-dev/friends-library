{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "jsx": "react",
    "types": ["node", "@emotion/core", "jest"],
    "esModuleInterop": true,
    "plugins": [
      {
        "name": "typescript-styled-plugin",
