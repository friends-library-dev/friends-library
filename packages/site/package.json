{
  "name": "gatsby-starter-default",
  "description": "Gatsby default starter",
  "version": "1.0.0",
  "author": "Kyle Mathews <mathews.kyle@gmail.com>",
  "dependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.6",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/preset-flow": "^7.0.0",
    "@babel/register": "^7.0.0",
    "@emotion/core": "^10.0.2",
    "@emotion/styled": "^10.0.2",
    "babel-plugin-ttag": "^1.4.2",
    "babel-preset-gatsby": "^0.1.6",
    "babel-preset-gatsby-package": "^0.1.3",
    "chalk": "^2.4.1",
    "emotion": "^10.0.2",
    "emotion-server": "^10.0.2",
    "emotion-theming": "^10.0.4",
    "gatsby": "^2.0.62",
    "gatsby-image": "^2.0.22",
    "gatsby-plugin-emotion": "^3.0.0",
    "gatsby-plugin-flow": "^1.0.2",
    "gatsby-plugin-manifest": "^2.0.11",
    "gatsby-plugin-offline": "^2.0.18",
    "gatsby-plugin-react-helmet": "^3.0.4",
    "gatsby-plugin-sharp": "^2.0.14",
    "gatsby-source-filesystem": "^2.0.10",
    "gatsby-transformer-sharp": "^2.1.9",
    "gettext-parser": "^3.1.0",
    "he": "^1.2.0",
    "moment": "^2.22.2",
    "react": "^16.7.0-alpha",
    "react-dom": "^16.7.0-alpha",
    "react-helmet": "^5.2.0",
    "ttag": "^1.4.2",
    "ttag-cli": "^1.4.2"
  },
  "keywords": [
    "gatsby"
  ],
  "resolutions": {
    "react-hot-loader": "4.5.1"
  },
  "license": "MIT",
  "scripts": {
    "cache:clear": "cp .cache/babelState.json . && rm -rf .cache && mkdir .cache && mv babelState.json .cache && rm -rf public",
    "build:en": "GATSBY_LANG=en gatsby build",
    "build:es": "GATSBY_LANG=es gatsby build",
    "develop:en": "DISABLE_V8_COMPILE_CACHE=1 GATSBY_LANG=en GATSBY_PORT=9222 gatsby develop --port 9222",
    "develop:es": "DISABLE_V8_COMPILE_CACHE=1 GATSBY_LANG=es GATSBY_PORT=9222 gatsby develop --port 9222",
    "format": "prettier --write \"src/**/*.js\"",
    "po:update": "yarn ttag update es.po src"
  },
  "devDependencies": {
    "prettier": "^1.15.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-default"
  }
}
