{
  "name": "@friends-library/ui",
  "version": "1.0.0",
  "description": "Shared react component library for Friends Library",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "author": "jared@netrivet.com",
  "license": "MIT",
  "scripts": {
    "compile": "cd ../../ && yarn compile ui",
    "storybook": "start-storybook -p 6006 --ci",
    "build": "build-storybook",
    "build:quiet": "build-storybook --loglevel error --quiet",
    "build:netlify": "yarn compile && yarn build:quiet",
    "test": "cd ../../ && yarn test packages/ui/src",
    "test:integration": "cd ../../ && yarn test --testRegex=packages/ui/.*/.*\\.integration\\.ts --env=node"
  },
  "dependencies": {
    "@emotion/core": "^10.0.14",
    "@emotion/styled": "^10.0.14",
    "@friends-library/cover-component": "1.0.0",
    "classnames": "^2.2.6",
    "emotion-theming": "^10.0.14",
    "gatsby-link": "^2.2.2",
    "react": "^16.9.0",
    "react-country-region-selector": "^1.4.3",
    "tailwindcss": "^1.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@friends-library/types": "1.0.0",
    "@storybook/addon-actions": "5.1.9",
    "@storybook/addon-centered": "5.1.9",
    "@storybook/addon-info": "5.1.9",
    "@storybook/addon-links": "5.1.9",
    "@storybook/addon-viewport": "5.1.9",
    "@storybook/addons": "5.1.9",
    "@storybook/react": "5.1.9",
    "@types/classnames": "^2.2.9",
    "@types/node-fetch": "^2.5.1",
    "@types/react": "^16.9.4",
    "@types/storybook__addon-centered": "^3.4.1",
    "@types/storybook__react": "^4.0.2",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.0.6",
    "file-loader": "^4.2.0"
  }
}
