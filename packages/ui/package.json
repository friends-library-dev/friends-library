{
  "name": "@friends-library/ui",
  "version": "1.0.0",
  "description": "Shared react component library for Friends Library",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "author": "jared@netrivet.com",
  "license": "MIT",
  "sideEffects": false,
  "scripts": {
    "make:waves": "node ./make-waves.js",
    "compile": "cd ../../ && yarn compile ui",
    "storybook": "node ./pre-build.js && start-storybook -p 6006 --ci",
    "start": "yarn storybook",
    "build": "node ./pre-build.js && build-storybook",
    "build:quiet": "node ./pre-build.js && build-storybook --loglevel error --quiet",
    "test": "cd ../../ && yarn test packages/ui/src",
    "test:integration": "cd ../../ && yarn test --config packages/ui/src/checkout/services/__tests__/jest.config.js --testRegex=packages/ui/.*/.*\\.integration\\.ts"
  },
  "dependencies": {
    "@friends-library/cover-component": "1.0.0",
    "@friends-library/env": "1.0.0",
    "@friends-library/friends": "1.0.0",
    "@friends-library/locale": "1.0.0",
    "@friends-library/lulu": "1.0.0",
    "@reach/accordion": "^0.8.0",
    "@reach/dialog": "^0.7.0",
    "algoliasearch": "^4.1.0",
    "classnames": "^2.2.6",
    "gatsby-background-image": "^1.0.1",
    "gatsby-image": "^2.3.2",
    "gatsby-link": "^2.2.2",
    "js-cookie": "^2.2.1",
    "react": "16.10.1",
    "react-country-region-selector": "2.1.0",
    "react-dom": "16.10.1",
    "react-focus-lock": "^2.3.1",
    "react-instantsearch-dom": "^6.4.0",
    "react-stripe-elements": "^6.0.1",
    "react-swipeable": "^5.5.1",
    "react-switch": "^5.0.1",
    "tailwindcss": "^1.2.0",
    "use-interval": "^1.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@friends-library/types": "1.0.0",
    "@storybook/addon-actions": "5.2.8",
    "@storybook/addon-centered": "5.2.8",
    "@storybook/addon-info": "5.2.8",
    "@storybook/addon-links": "5.2.8",
    "@storybook/addon-viewport": "5.2.8",
    "@storybook/addons": "5.2.8",
    "@storybook/react": "5.2.8",
    "@types/classnames": "^2.2.9",
    "@types/js-cookie": "^2.2.4",
    "@types/react": "16.9.11",
    "@types/react-instantsearch-dom": "^6.3.0",
    "@types/react-stripe-elements": "^6.0.0",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.0.6",
    "file-loader": "^4.2.0"
  }
}
